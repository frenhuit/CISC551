@model IEnumerable<Models.DemographicInformation>
@using GridMvc.Html

<h3>@ViewBag.Message</h3>

<div>
    @if (Model != null && Model.Count() > 0)
    {
        <div class="panel panel-primary pre-scrollable">
            @Html.Grid(Model).Named("EmployeeInformationData").Columns(columns =>
            {
                columns.Add(col => col.EmployeeId).Titled("Employee ID");
                columns.Add(col => col.EmployeeName).Titled("Employee Name");
                columns.Add(col => col.DOB).Titled("Date of Birth");
                columns.Add(col => col.Gender).Titled("Gender");
            }).WithPaging(2).Sortable()
        </div>
    }

</div>
